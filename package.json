{
  "name": "guide",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "scss": "sass --watch \"scss\":\"css\"",
    "scss:once": "sass \"scss\":\"css\"",
    "minify:css": "postcss \"css/styles.css\" -o \"dist/css/styles.min.css\" -w",
    "minify:js": "terser js/main.js -o dist/js/main.min.js --module -c drop_console=true -m && terser js/slider.js -o dist/js/slider.min.js --module -c drop_console=true -m && terser js/modal.js -o dist/js/modal.min.js --module -c drop_console=true -m && terser js/map.js -o dist/js/map.min.js --module -c drop_console=true -m && terser js/questionnaire.js -o dist/js/questionnaire.min.js --module -c drop_console=true -m",
    "watch:js": "chokidar \"js/**/*.js\" -c \"npm run minify:js\"",
    "dev": "concurrently --raw \"npm:scss\" \"npm:minify:css\" \"npm:watch:js\"",
    "build": "npm run scss:once && postcss \"css/styles.css\" -o \"dist/css/styles.min.css\" && npm run minify:js",
    "start": "node app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "cssnano": "^6.1.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.2",
    "sass": "^1.93.2",
    "terser": "^5.29.2",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "just-validate": "^4.3.0",
    "mongoose": "^8.14.1",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.10.0",
    "pg": "^8.14.0",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.3",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2"
  }
}
